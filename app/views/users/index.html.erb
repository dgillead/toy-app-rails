<h1>Current Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'View User', user %></td>
      <td><%= link_to 'Edit User', edit_user_path(user) %></td>
      <td><%= link_to 'Delete User', user, method: :delete, data: { confirm: 'Delete User?' } %></td>
    </tr>
  <% end %>
</table>

<br>

<%= link_to 'New User', new_user_path %>
